<Window x:Class="Rieltors.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rieltors"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1300" 
        ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200px"/>
            <ColumnDefinition Width="200px"/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="RegisterFrame" NavigationUIVisibility="Hidden" Margin="0,0,-800,0" Grid.ColumnSpan="2" />
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Grid.Column="1" Margin="0,0,-800,0" />
        <Frame x:Name="NavPanel" NavigationUIVisibility="Hidden" Grid.Column="0" Width="200"/>
    </Grid>
</Window>
